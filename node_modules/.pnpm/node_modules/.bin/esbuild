#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/indra/github/nomorkata/node_modules/.pnpm/esbuild@0.8.29/node_modules/esbuild/bin/esbuild/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/esbuild@0.8.29/node_modules/esbuild/bin/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/esbuild@0.8.29/node_modules/esbuild/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/esbuild@0.8.29/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules:/home/indra/github/nomorkata/node_modules:/home/indra/github/node_modules:/home/indra/node_modules:/home/node_modules:/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules/esbuild/bin/esbuild/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules/esbuild/node_modules"
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
