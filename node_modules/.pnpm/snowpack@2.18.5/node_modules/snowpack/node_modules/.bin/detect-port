#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/indra/github/nomorkata/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/bin/detect-port/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/bin/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/detect-port@1.3.0/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules:/home/indra/github/nomorkata/node_modules:/home/indra/github/node_modules:/home/indra/node_modules:/home/node_modules:/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/snowpack@2.18.5/node_modules/snowpack/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/snowpack@2.18.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../detect-port@1.3.0/node_modules/detect-port/bin/detect-port" "$@"
else 
  exec node  "$basedir/../../../../../detect-port@1.3.0/node_modules/detect-port/bin/detect-port" "$@"
fi
