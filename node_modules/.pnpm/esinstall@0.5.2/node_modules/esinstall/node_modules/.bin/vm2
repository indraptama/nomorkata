#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/indra/github/nomorkata/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/bin/vm2/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/bin/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/vm2@3.9.2/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules:/home/indra/github/nomorkata/node_modules:/home/indra/github/node_modules:/home/indra/node_modules:/home/node_modules:/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/esinstall@0.5.2/node_modules/esinstall/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/esinstall@0.5.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vm2@3.9.2/node_modules/vm2/bin/vm2" "$@"
else 
  exec node  "$basedir/../../../../../vm2@3.9.2/node_modules/vm2/bin/vm2" "$@"
fi
