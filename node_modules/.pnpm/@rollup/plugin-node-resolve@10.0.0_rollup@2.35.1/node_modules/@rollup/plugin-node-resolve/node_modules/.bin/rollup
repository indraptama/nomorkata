#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/indra/github/nomorkata/node_modules/.pnpm/rollup@2.35.1/node_modules/rollup/dist/bin/rollup/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/rollup@2.35.1/node_modules/rollup/dist/bin/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/rollup@2.35.1/node_modules/rollup/dist/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/rollup@2.35.1/node_modules/rollup/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/rollup@2.35.1/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/node_modules:/home/indra/github/nomorkata/node_modules:/home/indra/github/node_modules:/home/indra/node_modules:/home/node_modules:/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/@rollup/plugin-node-resolve@10.0.0_rollup@2.35.1/node_modules/@rollup/plugin-node-resolve/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/@rollup/plugin-node-resolve@10.0.0_rollup@2.35.1/node_modules/@rollup/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/@rollup/plugin-node-resolve@10.0.0_rollup@2.35.1/node_modules:/home/indra/github/nomorkata/node_modules/.pnpm/@rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rollup@2.35.1/node_modules/rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../../../../../../../rollup@2.35.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
